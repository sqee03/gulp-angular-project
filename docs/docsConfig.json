{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "./app/js",
            "./README.md"
        ]
    },
    "plugins": [
        "./node_modules/jsdoc/plugins/markdown"
    ],
    "categoryfile" : "./docs/categoryfile.json",
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/dist/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}